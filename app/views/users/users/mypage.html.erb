<%= link_to 'ユーザー情報編集', edit_users_path %>

<%= form_with model: @createList, local: true do |f| %>
  <label for="title">リスト名</label>
  <%= f.text_field :title, id: "title" %>
  <label for="about">概要</label>
  <%= f.text_area :about, id: "about" %>
  <%= f.submit "新規作成" %>
<% end %>
<% if @lists.count == 0 %>
単語リストがありません
<% else %>
  <% @lists.each do |list| %>
    <%= link_to "リスト名:#{list.title}", edit_list_path(list.id) %>
    概要：<%= list.about %>
    作成者<%= list.user_id %>
    ステータス<%= list.public_status %>
  <% end %>
<% end %>