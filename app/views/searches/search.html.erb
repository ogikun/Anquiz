<div class="container p-0 my-5">
  <div class="offset-md-2 col-md-8">
    <% if (params[:button] == "search_tag")||(params[:model] == "search_tag") %>
      <% if @results %>
        <h3>"<%= params[:keyword] %>"のタグ検索結果</h3>
        <%= render "layouts/list_index", lists: @results %>
      <% else %>
        <h3>検索結果はありません</h3>
      <% end %>
    <% else %>
      <% if @results %>
        <h3>"<%= params[:keyword] %>"を含むリスト名</h3>
        <%= render "layouts/list_index", lists: @results %>
      <% else %>
        <h3>検索結果はありません</h3>
      <% end %>
    <% end %>
  </div>
</div>
